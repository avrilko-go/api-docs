{
    "name": "tangwei/apidocs",
    "description": "A swagger library for Hyperf.",
    "license": "MIT",
    "keywords": [
      "php",
      "docs",
      "hyperf",
      "swagger",
      "hyperf swagger"
    ],
    "authors": [
      {
        "name": "TangWei",
        "email": "tw2066@163.com",
        "homepage": "https://github.com/tw2066",
        "role": "Developer"
      }
    ],
    "require": {
      "php": ">=8.0",
      "tangwei/dto": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\ApiDocs\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "hyperf": {
            "config": "Hyperf\\ApiDocs\\ConfigProvider"
        },
        "branch-alias":{
          "dev-master": "2.0.x-dev"
        }
    }
}
